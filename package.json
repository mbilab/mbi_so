    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gm": "^1.23.0",
    "html-webpack-plugin": "^2.22.0",
    "jquery": "^3.1.0",
    "livescript": "^1.5.0",
    "livescript-loader": "^0.1.5",
    "ls-lint": "^0.1.0",
